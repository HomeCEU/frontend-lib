{
  "name": "@homeceu/frontend-lib",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git://github.com/homeceu/frontend-lib.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@homeceu"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "start:ci": "ng serve --disable-host-check --host 0.0.0.0",
    "test": "ng test --code-coverage",
    "test:cypress:run": "npx cypress run",
    "test:cypress:ci": "start-server-and-test start:ci http://localhost:4200 test:cypress:run",
    "lint": "ng lint",
    "build:dts": "ng build --configuration=production --output-hashing none && node build-script.js"
  },
  "dependencies": {
    "@angular/animations": "~11.1.0",
    "@angular/cdk": "^11.1.0",
    "@angular/common": "~11.1.0",
    "@angular/compiler": "~11.1.0",
    "@angular/core": "~11.1.0",
    "@angular/elements": "^11.1.0",
    "@angular/forms": "~11.1.0",
    "@angular/material": "^11.1.0",
    "@angular/platform-browser": "~11.1.0",
    "@angular/platform-browser-dynamic": "~11.1.0",
    "@angular/router": "~11.1.0",
    "@swimlane/ngx-datatable": "^19.0.0",
    "@webcomponents/custom-elements": "^1.4.3",
    "rxjs": "~6.6.0",
    "subsink": "^1.0.2",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1101.1",
    "@angular/cli": "~11.1.1",
    "@angular/compiler-cli": "~11.1.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.14.22",
    "codelyzer": "^6.0.0",
    "concat": "^1.0.3",
    "cypress": "^6.3.0",
    "cypress-cucumber-preprocessor": "^4.0.0",
    "fs-extra": "^9.1.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~6.0.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "start-server-and-test": "^1.11.7",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.0",
    "typescript": "~4.1.3"
  }
}
