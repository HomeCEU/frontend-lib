{
  "name": "@homeceu/frontend-lib",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git://github.com/homeceu/frontend-lib.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@homeceu"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "start:ci": "ng serve --disable-host-check --host 0.0.0.0",
    "test": "ng test --code-coverage",
    "test:cypress:run": "npx cypress run",
    "test:cypress:ci": "start-server-and-test start:ci http://localhost:4200 test:cypress:run",
    "lint": "ng lint",
    "build:dts": "ng build --configuration=production --output-hashing none && node build-script.js"
  },
  "dependencies": {
    "@angular/animations": "~10.1.5",
    "@angular/cdk": "^10.2.4",
    "@angular/common": "~10.1.5",
    "@angular/compiler": "~10.1.5",
    "@angular/core": "~10.1.5",
    "@angular/elements": "^10.1.5",
    "@angular/forms": "~10.1.5",
    "@angular/material": "^10.2.4",
    "@angular/platform-browser": "~10.1.5",
    "@angular/platform-browser-dynamic": "~10.1.5",
    "@angular/router": "~10.1.5",
    "@swimlane/ngx-datatable": "^18.0.0",
    "@webcomponents/custom-elements": "^1.4.2",
    "rxjs": "~6.6.0",
    "subsink": "^1.0.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.6",
    "@angular/cli": "~10.1.6",
    "@angular/compiler-cli": "~10.1.5",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.67",
    "codelyzer": "^6.0.0",
    "concat": "^1.0.3",
    "cypress": "^6.2.1",
    "cypress-cucumber-preprocessor": "^4.0.0",
    "fs-extra": "^9.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "start-server-and-test": "^1.11.7",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
